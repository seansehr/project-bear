{
  "name": "project-bear",
  "version": "0.1.0",
  "description": "JS Build Process",
  "scripts": {
    "build": "browserify -d -t [ babelify --presets [ react es2015 ] ] main.js -o bundle.js | uglifyjs -cmo bundle.min.js -- bundle.js",
    "watch": "watchify -d -t [ babelify --presets [ react es2015 ] ] main.js -o bundle.js"
  },
  "author": "Sean Sehr",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "es6-collections": "*",
    "react": "0.14.7",
    "react-dom": "^0.14.7",
    "react-masonry-component": "^4.0.0",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.1.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {}
}
